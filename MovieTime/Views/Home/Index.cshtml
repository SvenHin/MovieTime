@model List<MovieTime.Models.Movie>

@{
    ViewBag.Title = "Index";
}
<head>
    <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/css/bootstrap.min.css">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <script src="jquery-1.11.2.js"></script>
    <script src="http://code.jquery.com/jquery-1.11.2.min.js"></script>

</head>

<style>
    .sidenav {
        height: 100%;
        width: 15%;
        position: fixed;
        z-index: 1;
        top: 0;
        left: 0;
        background-color: black;
        overflow-x: hidden;
        padding-top:20px;
        -ms-overflow-style: none;
    }
    .sidenav::-webkit-scrollbar {display: none;}

    .sidenav a {
        padding: 6px 6px 6px 6px;
        text-decoration: none;
        font-size: 20px;
        color: darkgrey;
        display: block;
    }
    .sidenav a:hover {
        color: lightgrey;
    }
    .main {
        margin-left: 15%;
    }

    @@media screen and (max-height: 450px) {
  .sidenav {padding-top: 15px;}
  .sidenav a {font-size: 18px;}
}

    .container {
        padding-left: 15%;
    }
</style>

<div class="sidenav">
    <a href="#"> Action </a>
    <a href="#"> Adventure </a>
    <a href="#"> Animation </a>
    <a href="#"> Children's </a>
    <a href="#"> Comedy </a>
    <a href="#"> Crime </a>
    <a href="#"> Documentary </a>
    <a href="#"> Drama </a>
    <a href="#"> Fantasy </a>
    <a href="#"> Film-Noir </a>
    <a href="#"> Horror </a>
    <a href="#"> Musical </a>
    <a href="#"> Mystery </a>
    <a href="#"> Romance </a>
    <a href="#"> Sci-Fi </a>
    <a href="#"> Thriller </a>
    <a href="#"> War </a>
    <a href="#"> Western </a>
</div>


<h2>Movie Time</h2>

<div class="container">
    <ul class="list-inline">
        @foreach (var item in Model)
        {
            <li class="list-inline-item">
                <input type="image" src="@item.ImageURL" alt="bildetest" />
            </li>
        }

    </ul>

</div>
<script type="text/JavaScript">
    $(function () {
        $(".list-inline-item").click(function () {
            alert("Hello World jQuery");
        }) 
    });
</script>





